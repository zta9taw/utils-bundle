{
    "name": "zta9taw/utils-bundle",
    "description": "Useful functionality for a symfony application.",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "symfony-bundle",
    "prefer-stable": true,
    "keywords": [
        "bundle",
        "zta9taw",
        "utils"
    ],
    "authors": [
        {
            "name": "abdelhak",
            "email": "abdelhak.freelance@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/orm-pack": "*",
        "symfony/polyfill-uuid": "*",
        "knplabs/knp-paginator-bundle": "^6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "symfony/test-pack": "*"
    },
    "autoload": {
        "exclude-from-classmap": [
            "Tests/"
        ],
        "psr-4": {
            "Zta9taw\\Bundle\\UtilsBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zta9taw\\Bundle\\UtilsBundle\\Tests\\": "Tests"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.2"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
